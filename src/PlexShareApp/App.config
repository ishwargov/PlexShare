<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.diagnostics>
		<trace autoflush="true" indentsize="4">
			<listeners>
				<add name="MyTraceListener"
				 type="MyNameSpace.MyCustomTraceListener, MyAssembly"
				traceOutputOptions="ProcessId, DateTime, Callstack" />
				<remove name="Default" />
			</listeners>
		</trace>
		
	</system.diagnostics>
</configuration>